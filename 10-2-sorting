
def bub(data):
    n=len(data)
    for i in range(n-1):
        for j in range(n-1,i,-1):
            if data[j-1]>data[j]:
                data[j-1], data[j]=data[j],data[j-1]
                


n=int(input())
data=[]
for i in range(n):
    s=input().split(' ')
    t=tuple([int(a) for a in s])
    data.append(t)
    
bub(data)

for k in data:
    print(f"{k[0]} {k[1]} {k[2]} ")
